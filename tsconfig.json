{
    "compilerOptions": {
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "Node",
        "skipLibCheck": true,
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "typeRoots": [
            "@types",
            "node_modules/@types"
        ],
    },
    "exclude": [
        "**/*.cy.ts"
    ]
}