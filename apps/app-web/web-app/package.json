{
    "name": "web-app",
    "version": "1.0.0",
    "description": "Navigation gauge and story generator with map component with customization options with data from a .gpx file and uploaded images.",
    "homepage": "https://nav-gauge.spookydoodle.com",
    "main": "src/index.tsx",
    "type": "module",
    "private": true,
    "license": "AGPL-3.0",
    "author": "spookydoodle",
    "funding": "https://ko-fi.com/spookydoodle",
    "repository": "https://github.com/spookydoodle/nav-gauge",
    "bugs": {
        "url": "https://github.com/spookydoodle/nav-gauge/issues"
    },
    "scripts": {
        "analyze": "source-map-explorer 'dist/static/js/*.js'",
        "dev": "rspack serve --mode development --config rspack.config.development.cjs",
        "start": "rspack serve --mode production --config rspack.config.production.cjs",
        "build": "tsc --noEmit && rspack --mode production --config rspack.config.production.cjs",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "clean": "npx shx rm -rf node_modules/",
        "test": "mocha",
        "test:e2e": "cypress run",
        "test:e2e:dev": "cypress run --config baseUrl=http://localhost:3000",
        "test:e2e:prod": "cypress run --config baseUrl=https://gauge.spookydoodle.com",
        "cypress": "cypress open"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "pmtiles": "^4.3.2",
        "react": "^19.2.3",
        "react-dom": "^19.2.3"
    },
    "devDependencies": {
        "@babel/core": "^7.28.6",
        "@babel/plugin-syntax-jsx": "^7.28.6",
        "@babel/plugin-syntax-typescript": "^7.28.6",
        "@rspack/cli": "^1.7.2",
        "@rspack/core": "^1.7.2",
        "@rspack/plugin-react-refresh": "^1.6.0",
        "@svgr/webpack": "^8.1.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.1",
        "@types/react": "^19.2.8",
        "@types/react-dom": "^19.2.3",
        "babel-loader": "^10.0.0",
        "babel-plugin-react-compiler": "^1.0.0",
        "css-loader": "^7.1.2",
        "cypress": "^15.9.0",
        "react-refresh": "^0.18.0",
        "style-loader": "^4.0.0",
        "ts-checker-rspack-plugin": "^1.2.3",
        "webpack-bundle-analyzer": "^5.1.1",
        "webpack-merge": "^6.0.1"
    },
    "resolutions": {
        "qs": "^6.14.1"
    }
}
