{
    "name": "nav-gauge",
    "version": "1.0.0",
    "description": "Navigation gauge and story generator with map component with customization options with data from a .gpx file and uploaded images.",
    "homepage": "https://nav-gauge.spookydoodle.com",
    "main": "src/index.tsx",
    "type": "module",
    "private": true,
    "license": "AGPL-3.0",
    "author": "spookydoodle",
    "funding": "https://ko-fi.com/spookydoodle",
    "repository": "https://github.com/spookydoodle/nav-gauge",
    "bugs": {
        "url": "https://github.com/spookydoodle/nav-gauge/issues"
    },
    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "dev": "rspack serve --mode development --config rspack.config.development.cjs",
        "build": "tsc --noEmit && rspack --mode production --config rspack.config.production.cjs",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "clean": "npx shx rm -rf node_modules/",
        "test:dev": "mocha",
        "test": "mocha --require ts-node/register \"src/test/**/*.test.ts\""
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@tmcw/togeojson": "^7.1.2",
        "@turf/bbox": "^7.2.0",
        "@turf/helpers": "^7.2.0",
        "@xmldom/xmldom": "^0.9.8",
        "classnames": "^2.5.1",
        "luxon": "3.7.1",
        "maplibre-gl": "5.6.1",
        "react": "19.1.1",
        "react-dom": "19.1.1"
    },
    "devDependencies": {
        "@rspack/cli": "^1.5.7",
        "@rspack/core": "^1.5.7",
        "@rspack/plugin-react-refresh": "^1.5.1",
        "@svgr/webpack": "^8.1.0",
        "@testing-library/react": "^16.3.0",
        "@types/chai": "^5.2.2",
        "@types/eslint": "^9.6.1",
        "@types/luxon": "^3.4.2",
        "@types/mocha": "^10.0.10",
        "@types/node": "^24.5.2",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@typescript-eslint/parser": "^8.38.0",
        "chai": "^6.2.0",
        "css-loader": "^7.1.2",
        "eslint": "^9.32.0",
        "eslint-plugin-react": "^7.37.5",
        "global-jsdom": "^27.0.0",
        "ignore-styles": "^5.0.1",
        "jsdom": "^27.0.0",
        "jsdom-global": "^3.0.2",
        "mocha": "^11.7.2",
        "path": "^0.12.7",
        "react-refresh": "^0.17.0",
        "style-loader": "^4.0.0",
        "ts-checker-rspack-plugin": "^1.1.5",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.6",
        "typescript": "5.8.3",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-merge": "^6.0.1"
    }
}
