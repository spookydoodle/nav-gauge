{
    "name": "nav-gauge",
    "version": "1.0.0",
    "description": "Navigation gauge and story generator with map component with customization options with data from a .gpx file and uploaded images.",
    "homepage": "https://nav-gauge.spookydoodle.com",
    "main": "src/index.tsx",
    "type": "module",
    "private": true,
    "license": "AGPL-3.0",
    "author": "spookydoodle",
    "funding": "https://ko-fi.com/spookydoodle",
    "repository": "https://github.com/spookydoodle/nav-gauge",
    "bugs": {
        "url": "https://github.com/spookydoodle/nav-gauge/issues"
    },
    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "dev": "rspack serve --mode development --config rspack.config.development.cjs",
        "start": "rspack serve --mode production --config rspack.config.production.cjs",
        "build": "tsc --noEmit && rspack --mode production --config rspack.config.production.cjs",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "clean": "npx shx rm -rf node_modules/",
        "test": "mocha",
        "test:e2e": "cypress run",
        "test:e2e:dev": "cypress run --config baseUrl=http://localhost:3000",
        "test:e2e:prod": "cypress run --config baseUrl=https://gauge.spookydoodle.com",
        "cypress": "cypress open"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@tmcw/togeojson": "^7.1.2",
        "@turf/along": "^7.2.0",
        "@turf/bbox": "^7.2.0",
        "@turf/bearing": "^7.2.0",
        "@turf/distance": "^7.2.0",
        "@turf/helpers": "^7.2.0",
        "@turf/length": "^7.2.0",
        "@xmldom/xmldom": "^0.8.11",
        "classnames": "^2.5.1",
        "exif-js": "^2.3.0",
        "luxon": "^3.7.2",
        "maplibre-gl": "^5.13.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.28.5",
        "@babel/plugin-syntax-jsx": "^7.27.1",
        "@babel/plugin-syntax-typescript": "^7.27.1",
        "@rspack/cli": "^1.6.3",
        "@rspack/core": "^1.6.3",
        "@rspack/plugin-react-refresh": "^1.5.3",
        "@svgr/webpack": "^8.1.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.0",
        "@types/chai": "^5.2.2",
        "@types/eslint": "^9.6.1",
        "@types/exif-js": "^2.3.1",
        "@types/luxon": "^3.7.1",
        "@types/mocha": "^10.0.10",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.5",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/eslint-plugin": "^8.46.4",
        "@typescript-eslint/parser": "^8.46.4",
        "babel-loader": "^10.0.0",
        "babel-plugin-react-compiler": "^1.0.0",
        "chai": "^6.2.0",
        "css-loader": "^7.1.2",
        "cypress": "^15.6.0",
        "eslint": "^9.39.1",
        "mocha": "^11.7.2",
        "path": "^0.12.7",
        "react-refresh": "^0.18.0",
        "style-loader": "^4.0.0",
        "ts-checker-rspack-plugin": "^1.2.0",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-merge": "^6.0.1"
    }
}
